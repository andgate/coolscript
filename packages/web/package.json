{
  "name": "coolscript-web",
  "private": true,
  "version": "0.0.0",
  "description": "Website for coolscript",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel -p 1234",
    "build": "parcel build --dist-dir ../../docs --no-source-maps --no-cache --no-content-hash --public-url ./",
    "check": "tsc --noEmit",
    "prettier": "prettier --check src/ tests/",
    "prettier:fix": "prettier --write src/ tests/",
    "lint": "eslint src/** tests/** --ext .ts,.tsx,.html",
    "lint:fix": "pnpm lint --fix",
    "verify": "pnpm prettier && pnpm lint",
    "verify:fix": "pnpm prettier:fix && pnpm lint:fix",
    "test": "jest",
    "clean": "rimraf coverage dist node_modules .pnpm-debug.log"
  },
  "author": "Gabriel Anderson",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.0.1"
  },
  "dependencies": {
    "language-coolscript": "^0.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
