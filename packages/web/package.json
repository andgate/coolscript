{
  "name": "coolscript-web",
  "private": true,
  "version": "0.0.4",
  "description": "Website for coolscript",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel --open",
    "build": "parcel build --dist-dir ../../docs --no-source-maps --no-cache --no-content-hash --public-url ./",
    "check": "tsc --noEmit",
    "prettier": "prettier --check src/ tests/",
    "prettier:fix": "prettier --write src/ tests/",
    "lint": "eslint src/** tests/** --ext .ts,.tsx,.html",
    "lint:fix": "pnpm lint --fix",
    "verify": "pnpm prettier && pnpm lint",
    "verify:fix": "pnpm prettier:fix && pnpm lint:fix",
    "test": "jest"
  },
  "author": "Gabriel Anderson",
  "license": "MIT",
  "devDependencies": {
    "@coolscript/backend-js": "^0.0.0",
    "@coolscript/eval": "^0.0.3",
    "@coolscript/parser": "^0.0.6",
    "@coolscript/syntax": "^0.0.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "csstips": "^1.2.0",
    "csx": "^10.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "typestyle": "^2.2.0"
  }
}
